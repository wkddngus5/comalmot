<article id="ssd">
  <table class="info">
    <tr>
      <th>
        <!-- Image card "ramWhat"-->
        <div class="demo-card-image mdl-card mdl-shadow--2dp" id="ssdWhatImg">
          <div class="mdl-card__title mdl-card--expand"></div>
          <div class="mdl-card__actions">
            <span class="demo-card-image__filename">SSD란?</span>
          </div>
        </div>
        <!-- <button onclick="dim();">Dim</button> -->
      </th>
      <th>
        <!-- Image card "ramPoint"-->
        <div class="demo-card-image mdl-card mdl-shadow--2dp" id="ssdPointImg">
          <div class="mdl-card__title mdl-card--expand"></div>
          <div class="mdl-card__actions">
            <span class="demo-card-image__filename">SSD주요 포인트!</span>
          </div>
        </div>
      </th>
    </tr>
  </table>
  <!-- Square card -->
  <hr>
  <table class="mdl-data-table mdl-js-data-table mdl-shadow--2dp">
    <thead>
    <tr>
      <th class="mdl-data-table__cell--non-numeric">이름</th>
      <th class="mdl-data-table__cell--non-numeric">제조사</th>
      <th class="mdl-data-table__cell--non-numeric">버퍼 용량</th>
      <th class="mdl-data-table__cell--non-numeric">인터페이스</th>
      <th class="mdl-data-table__cell--non-numeric">디스크 용량</th>
    </tr>
    </thead>
    <tbody>
      <% for(var i = 0 ; i < ssdList.length ; i++) {%>
        <tr>
          <td class="mdl-data-table__cell--non-numeric"><%= ssdList[i].name %></td>
          <td class="mdl-data-table__cell--non-numeric"><%= ssdList[i].company %></td>
          <td class="mdl-data-table__cell--non-numeric"><%= ssdList[i].details.bufferVolume %></td>
          <td class="mdl-data-table__cell--non-numeric"><%= ssdList[i].details.interface %></td>
          <td class="mdl-data-table__cell--non-numeric"><%= ssdList[i].details.diskVolume %></td>
        </tr>
      <% } %>
    </tbody>
  </table>
  <hr><br>


  <div class="demo-card-square mdl-card mdl-shadow--2dp" id='ssdWhatInfo' style="overflow:scroll; height:70%;">
    <div class="mdl-card__supporting-text" >
        <% for(var i=0; i<informationsList.length; i++) {%>
          <% if(informationsList[i].name=="ssdWhat") { %>
            <h3><%= informationsList[i].title %></h3>
            <p><%= informationsList[i].contents %></p>
          <% } %>
        <% } %>
    </div>
  </div>


  <div class="demo-card-square mdl-card mdl-shadow--2dp" id='ssdPointInfo' style="overflow:scroll; height:70%;">
    <!-- <div id="" style="overflow:scroll; height:400px;"> -->
    <div class="mdl-card__supporting-text">
        <% for(var i=0; i<informationsList.length; i++) {%>
          <% if(informationsList[i].name=="ssdPoint") { %>
            <h3><%= informationsList[i].title %></h3>
            <p><%= informationsList[i].contents %></p>
          <% } %>
        <% } %>
    </div>
  </div>

</article>
