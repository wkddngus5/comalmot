<article id="ram">
  <table class="info">
    <tr>
      <th>
          <!-- Image card "mainBoardWhat"-->
        <div class="demo-card-image mdl-card mdl-shadow--2dp"  id="mainBoardWhatImg">
          <div class="mdl-card__title mdl-card--expand"></div>
          <div class="mdl-card__actions">
            <span class="demo-card-image__filename">Main board란?</span>
          </div>
        </div>
      </th>
      <th>
        <!-- Image card "mainBoardPoint"-->
        <div class="demo-card-image mdl-card mdl-shadow--2dp" id="mainBoardPointImg">
          <div class="mdl-card__title mdl-card--expand"></div>
          <div class="mdl-card__actions">
            <span class="demo-card-image__filename">주요 포인트!</span>
          </div>
        </div>
      </th>
    </tr>
  </table>
<!-- Square card -->
<hr>
<table class="mdl-data-table mdl-js-data-table mdl-shadow--2dp">
  <thead>
    <tr>
      <th class="mdl-data-table__cell--non-numeric">이름</th>
      <th class="mdl-data-table__cell--non-numeric">제조사</th>
      <th class="mdl-data-table__cell--non-numeric">CPU 소켓</th>
      <th class="mdl-data-table__cell--non-numeric">캐시 메모리</th>
      <th class="mdl-data-table__cell--non-numeric">그래픽 카드 슬롯</th>
    </tr>
  </thead>
  <tbody>
    <% for(var i = 0 ; i < mainBoardList.length ; i++) {%>
      <tr>
        <td class="mdl-data-table__cell--non-numeric"><%= mainBoardList[i].name %></td>
        <td class="mdl-data-table__cell--non-numeric"><%= mainBoardList[i].company %></td>
        <td class="mdl-data-table__cell--non-numeric"><%= mainBoardList[i].details.cpuSocket %></td>
        <td class="mdl-data-table__cell--non-numeric"><%= mainBoardList[i].details.cacheMemory %></td>
        <td class="mdl-data-table__cell--non-numeric"><%= mainBoardList[i].details.graphicCardSlot %></td>
      </tr>
    <% } %>
  </tbody>
</table>
<hr><br>


<div class="demo-card-square mdl-card mdl-shadow--2dp" id='ramWhatInfo' style="overflow:scroll; height:70%;">
    <div class="mdl-card__supporting-text" >
      <% for(var i=0; i<informationsList.length; i++) {%>
        <% if(informationsList[i].name=="ramWhat") { %>
          <h3><%= informationsList[i].title %></h3>
          <p><%= informationsList[i].contents %></p>
        <% } %>
      <% } %>
    </div>
</div>


<div class="demo-card-square mdl-card mdl-shadow--2dp" id='ramPointInfo' style="overflow:scroll; height:70%;">
  <!-- <div id="" style="overflow:scroll; height:400px;"> -->
  <div class="mdl-card__supporting-text">
    <% for(var i=0; i<informationsList.length; i++) {%>
      <% if(informationsList[i].name=="ramPoint") { %>
        <h3><%= informationsList[i].title %></h3>
        <p><%= informationsList[i].contents %></p>
      <% } %>
    <% } %>
  </div>
</div>

</article>
